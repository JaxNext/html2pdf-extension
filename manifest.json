{
  "manifest_version": 3,
  "name": "HTML to PDF Converter",
  "version": "1.0",
  "description": "Convert any webpage to PDF",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Convert to PDF"
  },
  "background": {
    "service_worker": "background.js"
  }
} 